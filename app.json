{
  "expo": {
    "name": "Periodic Comprehension E-Reader",
    "slug": "periodic-comprehension-e-reader",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.periodiccomprehension.ereader"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "periodic-comprehension",
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
